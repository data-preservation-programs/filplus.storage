{
  "seo": {
    "title": "General Application — Filecoin Plus",
    "description": "General DataCap application form with a selected notary."
  },
  "og": {
    "site_name": "General Application — Filecoin Plus",
    "url": "",
    "type": "website",
    "image": "/open-graph-image.jpg"
  },
  "page_content": {
    "hero": {
      "label": "General Application",
      "heading": "Fill out an application to send to your selected Notary."
    },
    "form": {
      "heading": "General DataCap Notary Review Request",
      "restore_saved_form_button_text": "Restore Saved Form",
      "submit_button_text": "Send Request",
      "scaffold": {
        "organization_name": {
          "field_key": "organization_name",
          "type": "input",
          "input_type": "text",
          "model_key": "organization_name",
          "label": "Organization Name",
          "placeholder": "ABC Inc.",
          "tooltip": "Provide the name of your group or organization.",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validation_message": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "default_value": ""
        },
        "organization_website": {
          "field_key": "organization_website",
          "type": "input",
          "input_type": "text",
          "model_key": "organization_website",
          "label": "Organization Website",
          "placeholder": "https://foo.com",
          "tooltip": "Enter the full URL of your group or organization's website.",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validation_message": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "default_value": ""
        },
        "ga_region": {
          "field_key": "ga_region",
          "type": "select",
          "model_key": "ga_region",
          "label": "Region",
          "required": true,
          "tooltip": "",
          "output": "option",
          "validation_message": {
            "required": "This field is required"
          },
          "options": [
            { "label": "North America" },
            { "label": "Africa" },
            { "label": "Oceania" },
            { "label": "Europe" },
            { "label": "Asia excl. GCN" },
            { "label": "Asia excl. Greater China" },
            { "label": "Asia excl. Japan" },
            { "label": "Asia Japan" },
            { "label": "Greater China" },
            { "label": "None" }
          ],
          "default_value": ""
        },
        "organization_social_media_handle": {
          "field_key": "organization_social_media_handle",
          "type": "input",
          "input_type": "text",
          "model_key": "organization_social_media_handle",
          "label": "Organization Social Media Handle",
          "placeholder": "@foo",
          "tooltip": "Provide the social media handle or full social media URL for your organization or group",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validation_message": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "default_value": ""
        },
        "organization_social_media_handle_type": {
          "field_key": "organization_social_media_handle_type",
          "type": "input",
          "input_type": "text",
          "model_key": "organization_social_media_handle_type",
          "label": "Social Media Type",
          "placeholder": "Twitter",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 100
          },
          "validation_message": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "default_value": ""
        },
        "total_datacap_size_input": {
          "field_key": "total_datacap_size_input",
          "type": "input",
          "input_type": "number",
          "model_key": "total_datacap_size",
          "label": "Total DataCap being requested",
          "placeholder": "32",
          "tooltip": "Provide the total amount of DataCap you're requesting, including replication. Use this field to select the amount and the adjecent field to select a unit.",
          "min": 1,
          "max": 5242880,
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "mirror": {
            "field_key": "total_datacap_size_range",
            "primary": true
          },
          "transform": {
            "func": "$formatDatacapSize",
            "args": {
              "action": "human"
            }
          },
          "autocomplete": "none",
          "validation_message": {
            "required": "This field is required",
            "minmax": "Number must be between 1 and 5242880"
          },
          "default_value": ""
        },
        "total_datacap_size_unit": {
          "field_key": "total_datacap_size_unit",
          "type": "select",
          "model_key": "total_datacap_size_unit",
          "label": "Unit",
          "tooltip": "Select a unit of measurement for the amount of DataCap you're requesting.",
          "required": true,
          "output": "option",
          "react": {
            "field_key": "total_datacap_size_range",
            "func": "$reactDatasizeUnitToRange",
            "args": {
              "value_from_field": "total_datacap_size_range"
            }
          },
          "validation_message": {
            "required": "This field is required"
          },
          "options": [
            { "label": "GiB" },
            { "label": "TiB" },
            { "label": "PiB" }
          ],
          "default_value": ""
        },
        "filecoin_address": {
          "field_key": "filecoin_address",
          "type": "input",
          "input_type": "text",
          "model_key": "filecoin_address",
          "label": "Filecoin Address",
          "placeholder": "f...",
          "tooltip": "Provide your Filecoin wallet address, which will usually begin with f1 or f3 for addresses on the mainnet",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validation_message": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "default_value": ""
        }
      }
    }
  }
}
