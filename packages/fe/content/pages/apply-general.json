{
  "seo": {
    "title": "General Application — Filecoin Plus",
    "description": "General DataCap application form with a selected notary."
  },
  "og": {
    "site_name": "General Application — Filecoin Plus",
    "url": "",
    "type": "website",
    "image": "/open-graph-image.png"
  },
  "page_content": {
    "hero": {
      "label": "General Application",
      "heading": "Send an application for DataCap to the notary you selected<br /><span class='highlight'>|notary|</span>",
      "kyc": {
        "heading": "Lorem ipsum dolor sit amet verification program",
        "button": {
          "unverified": {
            "text": "Get verified (optional)",
            "tooltip": "Optionally, you can get DataCap even faster by performing KYC verification through Togggle. KYC is not required. However, applicants that have passed KYC are more likely to be approved quickly. You’ll only need to perform KYC once. It will be applied to every application you make moving forward. |link|"
          },
          "verifying": {
            "text": "Verifying...",
            "tooltip": "It looks like your KYC verification is currently in progress. |link|"
          },
          "failure": {
            "text": "Verification not available",
            "tooltip": "You are not eligible for KYC verification at this time. If you feel that this is a mistake, please |link| to the Filecoin plus team."
          },
          "success": {
            "text": "Verified User",
            "tooltip": "Thanks for being a verified user of the Filecoin Plus program. Being verified means faster approvals and shorter time to DataCap."
          }
        }
      }
    },
    "back_button": {
      "type": "nuxt-link",
      "href": "/apply/general/notaries",
      "label": "Select a different notary",
      "theme": "pink",
      "icon": "chevron"
    },
    "form": {
      "heading": "General DataCap Notary Review Request",
      "restore_saved_form_button_text": "Restore Saved Form",
      "submit_button_text": "Send Request",
      "github_issue_link_text": "View Issue on Github",
      "scaffold": {
        "organization_name": {
          "type": "input",
          "inputType": "text",
          "modelKey": "organization_name",
          "label": "Organization Name",
          "placeholder": "ABC Inc.",
          "tooltip": "Provide the name of your group or organization.",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validationMessage": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "defaultValue": ""
        },
        "organization_website": {
          "type": "input",
          "inputType": "text",
          "modelKey": "organization_website",
          "label": "Organization Website",
          "placeholder": "https://foo.com",
          "tooltip": "Enter the full URL of your group or organization's website.",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validationMessage": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "defaultValue": ""
        },
        "ga_region": {
          "type": "select",
          "modelKey": "ga_region",
          "label": "Region",
          "required": true,
          "tooltip": "",
          "output": "option",
          "validationMessage": {
            "required": "This field is required"
          },
          "options": [
            { "label": "North America" },
            { "label": "Africa" },
            { "label": "Oceania" },
            { "label": "Europe" },
            { "label": "Asia excl. GCN" },
            { "label": "Asia excl. Greater China" },
            { "label": "Asia excl. Japan" },
            { "label": "Asia Japan" },
            { "label": "Greater China" },
            { "label": "None" }
          ],
          "defaultValue": ""
        },
        "organization_social_media_handle": {
          "type": "input",
          "inputType": "text",
          "modelKey": "organization_social_media_handle",
          "label": "Organization Social Media Handle",
          "placeholder": "@foo",
          "tooltip": "Provide the social media handle or full social media URL for your organization or group",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validationMessage": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "defaultValue": ""
        },
        "organization_social_media_handle_type": {
          "type": "input",
          "inputType": "text",
          "modelKey": "organization_social_media_handle_type",
          "label": "Social Media Type",
          "placeholder": "Twitter",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 100
          },
          "validationMessage": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "defaultValue": ""
        },
        "total_datacap_size_input": {
          "type": "input",
          "inputType": "number",
          "modelKey": "total_datacap_size",
          "label": "Total Datacap being requested",
          "placeholder": "32",
          "min": 1,
          "max": 5242880,
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "mirror": {
            "fieldKey": "total_datacap_size_range",
            "primary": true,
            "func": "$formatDatacapSize",
            "args": {
              "value_from_field": "total_datacap_size_range",
              "action": "human"
            }
          },
          "autocomplete": "off",
          "validationMessage": {
            "required": "This field is required",
            "minmax": "Number must be between 1 and 5242880"
          },
          "defaultValue": 32
        },
        "total_datacap_size_unit": {
          "type": "select",
          "modelKey": "total_datacap_size_unit",
          "label": "Unit",
          "required": true,
          "output": "option",
          "react": {
            "fieldKey": "total_datacap_size_range",
            "func": "$reactDatasizeUnitToRange",
            "args": {
              "value_from_field": "total_datacap_size_range"
            }
          },
          "validationMessage": {
            "required": "This field is required"
          },
          "options": [
            { "label": "GiB" },
            { "label": "TiB" },
            { "label": "PiB" }
          ],
          "defaultValue": "GiB"
        },
        "filecoin_address": {
          "type": "input",
          "inputType": "text",
          "modelKey": "filecoin_address",
          "label": "Filecoin Address",
          "placeholder": "f...",
          "tooltip": "Provide your Filecoin wallet address, which will usually begin with f1 or f3 for addresses on the mainnet",
          "required": true,
          "pre": "[^\\u0000-\\u00ff]",
          "autocomplete": "none",
          "chars": {
            "min": 2,
            "max": 1000
          },
          "validationMessage": {
            "required": "This field is required",
            "chars": "Input must be between 2 and 35 characters"
          },
          "defaultValue": ""
        }
      }
    }
  }
}
